    <style name="feedback_text_style">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="feedback_type_style" parent="@style/feedback_text_style">
        <item name="android:paddingLeft">27.0dip</item>
        <item name="android:layout_marginRight">15.0dip</item>
        <item name="android:button">@drawable/more_feedback_type_radiobutton</item>
